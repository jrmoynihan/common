{
	"name": "@jrmoynihan/common",
	"version": "6.1.0",
	"repository": "github:jrmoynihan/common",
	"author": "James Moynihan",
	"scripts": {
		"dev": "vite dev",
		"build package": "svelte-kit sync && svelte-package",
		"package-publish-patch": "pnpm version patch && svelte-package && cd package && pnpm publish",
		"package-publish-minor": "pnpm version minor && svelte-package && cd package && pnpm publish",
		"package-publish-major": "pnpm version major && svelte-package && cd package && pnpm publish",
		"prepublishOnly": "echo 'Did you mean to publish `./package/`, instead of `./`?' && exit 1",
		"publish": "cd package && pnpm publish",
		"test": "playwright test",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check --plugin-search-dir=. . && eslint .",
		"format": "prettier --write --plugin-search-dir=. ."
	},
	"devDependencies": {
		"@playwright/test": "^1.29.2",
		"@sveltejs/adapter-auto": "^1.0.2",
		"@sveltejs/kit": "^1.2.2",
		"@sveltejs/package": "^1.0.2",
		"@typescript-eslint/eslint-plugin": "^5.48.2",
		"@typescript-eslint/parser": "^5.48.2",
		"eslint": "^8.32.0",
		"eslint-config-prettier": "^8.6.0",
		"eslint-plugin-svelte": "^2.14.1",
		"eslint-plugin-svelte3": "^4.0.0",
		"mdsvex": "^0.10.6",
		"prettier": "^2.8.3",
		"prettier-plugin-svelte": "^2.9.0",
		"remark-codesandbox": "^0.10.1",
		"sass": "^1.57.1",
		"svelte-check": "^3.0.2",
		"svelte-preprocess": "^5.0.1",
		"svelte2tsx": "^0.6.0",
		"tslib": "^2.4.1",
		"typescript": "^4.9.4",
		"vite": "^4.0.4",
		"vite-plugin-mkcert": "^1.11.0"
	},
	"type": "module",
	"dependencies": {
		"svelte": "^3.55.1",
		"@fortawesome/fontawesome-common-types": "^6.2.1",
		"@fortawesome/free-solid-svg-icons": "^6.2.1",
		"@jrmoynihan/svelte-fa": "^3.0.6",
		"@js-temporal/polyfill": "^0.4.3",
		"@zerodevx/svelte-toast": "^0.8.2",
		"mdsvexamples": "^0.3.3"
	}
}
