{
	"name": "@jrmoynihan/common",
	"version": "2.2.0",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"package": "svelte-kit package",
		"package-publish-patch": "pnpm version patch && svelte-kit package && cd package && pnpm version patch --force && pnpm publish",
		"package-publish-minor": "pnpm version minor && svelte-kit package && cd package && pnpm version minor --force && pnpm publish",
		"package-publish-major": "pnpm version major && svelte-kit package && cd package && pnpm version major --force && pnpm publish",
		"publish": "cd package && pnpm publish",
		"test": "playwright test",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check --plugin-search-dir=. . && eslint .",
		"format": "prettier --write --plugin-search-dir=. ."
	},
	"devDependencies": {
		"@playwright/test": "^1.22.2",
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/kit": "next",
		"@typescript-eslint/eslint-plugin": "^5.27.0",
		"@typescript-eslint/parser": "^5.27.0",
		"eslint": "^8.16.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"prettier": "^2.6.2",
		"prettier-plugin-svelte": "^2.7.0",
		"sass": "^1.54.3",
		"svelte": "^3.44.0",
		"svelte-check": "^2.7.1",
		"svelte-preprocess": "^4.10.6",
		"svelte2tsx": "^0.5.13",
		"tslib": "^2.3.1",
		"typescript": "^4.7.4",
		"vite": "^3.0.0"
	},
	"type": "module",
	"dependencies": {
		"@fortawesome/free-solid-svg-icons": "^6.1.2",
		"@js-temporal/polyfill": "^0.4.2",
		"svelte-fa": "^3.0.3"
	}
}
