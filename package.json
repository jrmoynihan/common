{
	"name": "@jrmoynihan/common",
	"version": "6.1.2",
	"repository": "github:jrmoynihan/common",
	"author": "James Moynihan",
	"scripts": {
		"dev": "vite dev",
		"build package": "svelte-kit sync && svelte-package",
		"package-publish-patch": "pnpm version patch && svelte-package && cd package && pnpm publish",
		"package-publish-minor": "pnpm version minor && svelte-package && cd package && pnpm publish",
		"package-publish-major": "pnpm version major && svelte-package && cd package && pnpm publish",
		"prepublishOnly": "echo 'Did you mean to publish `./package/`, instead of `./`?' && exit 1",
		"publish": "cd package && pnpm publish",
		"test": "playwright test",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check --plugin-search-dir=. . && eslint .",
		"format": "prettier --write --plugin-search-dir=. ."
	},
	"devDependencies": {
		"@playwright/test": "^1.30.0",
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/adapter-vercel": "^2.0.2",
		"@sveltejs/kit": "^1.5.5",
		"@sveltejs/package": "^1.0.2",
		"@typescript-eslint/eslint-plugin": "^5.51.0",
		"@typescript-eslint/parser": "^5.51.0",
		"eslint": "^8.34.0",
		"eslint-config-prettier": "^8.6.0",
		"eslint-plugin-svelte": "^2.18.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"mdsvex": "^0.10.6",
		"prettier": "^2.8.4",
		"prettier-plugin-svelte": "^2.9.0",
		"remark-codesandbox": "^0.10.1",
		"sass": "^1.58.0",
		"svelte-check": "^3.0.3",
		"svelte-preprocess": "^5.0.1",
		"svelte2tsx": "^0.6.1",
		"tslib": "^2.5.0",
		"typescript": "^4.9.5",
		"vite": "^4.1.1",
		"vite-plugin-mkcert": "^1.13.0"
	},
	"type": "module",
	"dependencies": {
		"@fortawesome/fontawesome-common-types": "^6.3.0",
		"@fortawesome/free-solid-svg-icons": "^6.3.0",
		"@jrmoynihan/svelte-fa": "^3.0.6",
		"@js-temporal/polyfill": "^0.4.3",
		"@zerodevx/svelte-toast": "^0.8.2",
		"mdsvexamples": "^0.3.3",
		"svelte": "^3.55.1"
	}
}
