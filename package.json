{
	"name": "@jrmoynihan/common",
	"version": "2.9.10",
	"repository": "github:jrmoynihan/common",
	"author": "James Moynihan",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"package": "svelte-package",
		"package-publish-patch": "pnpm version patch && svelte-package && cd package && pnpm publish",
		"package-publish-minor": "pnpm version minor && svelte-package && cd package && pnpm publish",
		"package-publish-major": "pnpm version major && svelte-package && cd package && pnpm publish",
		"publish": "cd package && pnpm publish",
		"test": "playwright test",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check --plugin-search-dir=. . && eslint .",
		"format": "prettier --write --plugin-search-dir=. ."
	},
	"devDependencies": {
		"@playwright/test": "^1.25.2",
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/kit": "next",
		"@sveltejs/package": "1.0.0-next.3",
		"@typescript-eslint/eslint-plugin": "^5.36.2",
		"@typescript-eslint/parser": "^5.36.2",
		"eslint": "^8.23.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"mdsvex": "^0.10.6",
		"prettier": "^2.7.1",
		"prettier-plugin-svelte": "^2.7.0",
		"remark-codesandbox": "^0.10.1",
		"sass": "^1.54.9",
		"svelte": "^3.50.1",
		"svelte-check": "^2.9.0",
		"svelte-preprocess": "^4.10.7",
		"svelte2tsx": "^0.5.16",
		"tslib": "^2.4.0",
		"typescript": "^4.8.3",
		"vite": "3.1.0",
		"vite-plugin-mkcert": "^1.9.0"
	},
	"type": "module",
	"dependencies": {
		"@fortawesome/fontawesome-common-types": "^6.2.0",
		"@fortawesome/free-solid-svg-icons": "^6.2.0",
		"@js-temporal/polyfill": "^0.4.2",
		"@zerodevx/svelte-toast": "^0.7.2",
		"mdsvexamples": "^0.3.1",
		"svelte-fa": "^3.0.3"
	}
}
