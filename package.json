{
	"name": "@jrmoynihan/common",
	"version": "8.0.3",
	"repository": "github:jrmoynihan/common",
	"author": "James Moynihan",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"build package": "svelte-kit sync && svelte-package",
		"package-publish-patch": "pnpm version patch && svelte-package && cd package && pnpm publish",
		"package-publish-minor": "pnpm version minor && svelte-package && cd package && pnpm publish",
		"package-publish-major": "pnpm version major && svelte-package && cd package && pnpm publish",
		"prepublishOnly": "echo 'Did you mean to publish `./package/`, instead of `./`?' && exit 1",
		"publish": "cd package && pnpm publish",
		"test": "playwright test",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check --plugin-search-dir=. . && eslint .",
		"format": "prettier --write --plugin-search-dir=. ."
	},
	"devDependencies": {
		"@playwright/test": "^1.33.0",
		"@sveltejs/adapter-auto": "^2.0.1",
		"@sveltejs/package": "^2.0.2",
		"@typescript-eslint/eslint-plugin": "^5.59.2",
		"@typescript-eslint/parser": "^5.59.2",
		"eslint": "^8.40.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-svelte": "^2.27.3",
		"mdsvex": "^0.10.6",
		"prettier": "^2.8.8",
		"prettier-plugin-svelte": "^2.10.0",
		"remark-codesandbox": "^0.10.1",
		"sass": "^1.62.1",
		"svelte-check": "^3.3.1",
		"svelte-preprocess": "^5.0.3",
		"svelte2tsx": "^0.6.13",
		"tslib": "^2.5.0",
		"typescript": "^5.0.4",
		"vite": "^4.3.5",
		"vite-plugin-mkcert": "^1.15.0"
	},
	"type": "module",
	"dependencies": {
		"@fortawesome/fontawesome-common-types": "^6.4.0",
		"@fortawesome/free-solid-svg-icons": "^6.4.0",
		"@jrmoynihan/svelte-fa": "^3.0.6",
		"@js-temporal/polyfill": "^0.4.3",
		"@sveltejs/kit": "^1.16.2",
		"@zerodevx/svelte-toast": "^0.9.3",
		"mdsvexamples": "^0.3.3",
		"svelte": "^3.59.0"
	}
}
